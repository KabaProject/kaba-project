generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model gallery {
  id         Int    @id @default(autoincrement())
  file       String
  project_id String
}

model projects {
  id          String  @id
  name        String
  description String?
  repo        String?
  link        String?
  type_id     Int
  types       types   @relation(fields: [type_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model types {
  id       Int        @id @default(autoincrement())
  name     String
  projects projects[]
}
